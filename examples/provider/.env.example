# Atlassian Operations Provider Configuration
# Copy this file to .env and fill in your actual values

# Atlassian Cloud ID
TF_VAR_atlassian_cloud_id=xxxxxxxx-xxxx-xxxx-xxxx-xxxxxxxxxxxx

# Atlassian domain name (e.g., yourcompany.atlassian.net)
TF_VAR_atlassian_domain_name=yourcompany.atlassian.net

# Email address for your Atlassian account
TF_VAR_atlassian_email_address=email@example.com

# API token created in Atlassian account settings
TF_VAR_atlassian_token=your_api_token_here

# NON-SCOPED API Token created in Organization administration (only required for Compass)
TF_VAR_atlassian_org_admin_token=your_org_admin_token_here

# Atlassian operations product type (jira-service-desk or compass)
TF_VAR_atlassian_product_type=jira-service-desk
